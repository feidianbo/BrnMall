@model BrnMall.PayPlugin.ChinaBank.ConfigModel
           
<script type="text/javascript" src="/admin_mall/scripts/jquery.validate.js"></script>
<script type="text/javascript" src="/admin_mall/scripts/jquery.validate.unobtrusive.js"></script>
<h1 class="rightH1">
插件管理 &gt;&gt; 插件配置
<div class="right">
<div class="clear"></div>
</div>
<div class="clear"></div>
</h1>
@using (Html.BeginForm("config", "adminchinabank"))
{
<div class="addTable">
<table width="100%">
  <tr>
    <td width="100px" align="right">商户编号：</td>
    <td>
    @Html.TextBoxFor(x => x.Mid, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.Mid)
    </td>
    </tr>
      <tr>
    <td align="right">MD5私钥值：</td>
    <td>
    @Html.TextBoxFor(x => x.Key, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.Key)
    </td>
    </tr>
          <tr>
    <td align="right">支付手续费：</td>
    <td>
    @Html.TextBoxFor(x => x.PayFee, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.PayFee)
    </td>
    </tr>
      <tr>
    <td align="right">免费金额：</td>
    <td>
    @Html.TextBoxFor(x => x.FreeMoney, new { @class="input",size="20"})
    @Html.ValidationMessageFor(x => x.FreeMoney)
    </td>
    </tr>
  <tr>
    <td align="right">&nbsp;</td>
    <td>
    <a href="" class="addBt" id="sumbitBut" ><img src="/admin_mall/images/submit.jpg" /></a>
            <script type="text/javascript">
                $(function () {
                    $("#sumbitBut").click(function () {
                        $("form:first").submit();
                        return false;
                    })
                });
        </script>
    </td>
    </tr>
  <tr>
    <td align="right">&nbsp;</td>
    <td>&nbsp;</td>
    </tr>
</table>
</div>
}
@model BrnMall.Web.Models.SubmitResultModel
@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>订单提交结果-@{@WorkContext.MallConfig.SiteTitle}</title>
    @Html.Raw(WorkContext.MallConfig.SEOKeyword)
    @Html.Raw(WorkContext.MallConfig.SEODescription)
    <link href="@{@WorkContext.CSSCDN}/css/base.css" rel="stylesheet" type="text/css" />
    <link href="@{@WorkContext.CSSCDN}/css/content.css" rel="stylesheet" type="text/css" />
    <script src="@{@WorkContext.ScriptCDN}/scripts/jquery.js" type="text/javascript"></script>
    <script src="@{@WorkContext.ScriptCDN}/scripts/common.js" type="text/javascript"></script>
    <script type="text/javascript">
        uid = @WorkContext.Uid;
        isGuestSC = @WorkContext.MallConfig.IsGuestSC;
        scSubmitType = @WorkContext.MallConfig.SCSubmitType;
    </script>
</head>

<body>
@Html.Partial("_header")

<div class="buyTip box">
<div class="buyTipInner">
<img src="@{@WorkContext.ImageCDN}/images/ok.gif" width="43" height="48" /> 
    @if (Model.AllPayMoney > 0)
    {
        if (((IPayPlugin)Model.PayPlugin.Instance).PayMode == 1)
        {
        @:提交成功,支付方式为@{@Model.PayPlugin.FriendlyName} <a href="@Url.Action("payshow", new RouteValueDictionary { { "oidList", Model.OnlinePayOidList } })"  class="redBT">点击前往支付</a>
        }
        else
        {
        @:提交成功,支付方式为@{@Model.PayPlugin.FriendlyName},请您尽快付款
        }
    }
    else
    { 
    @:您的订单已经提交成功,我们会尽快为您发货
    }
</div>
</div>

@Html.Partial("_footer")
</body>
</html>


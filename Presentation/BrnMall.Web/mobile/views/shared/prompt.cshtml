@model BrnMall.Web.Framework.PromptModel
@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>消息提示-@{@WorkContext.MallConfig.SiteTitle}触屏版</title>
    @Html.Raw(WorkContext.MallConfig.SEOKeyword)
    @Html.Raw(WorkContext.MallConfig.SEODescription)
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link href="@{@WorkContext.CSSCDN}/mobile/css/base.css" rel="stylesheet" type="text/css">
    <script src="@{@WorkContext.ScriptCDN}/mobile/scripts/jqeury.js" type="text/javascript"></script>
    <script src="@{@WorkContext.ScriptCDN}/mobile/scripts/common.js" type="text/javascript"></script>
</head>

<body style="background:#fff;">
@Html.Partial("_header")
<script type="text/javascript">
    $("#headerTitle").html("消息提示");
</script>

<div style="padding:40px; font-size:20px; text-align:center;">@Model.Message</div>


</body>
</html>